#! /bin/bash

clear

echo "Looking for LOCK files, output tee to stdout and '/tmp/LOCK-FILES.txt'"
touch /tmp/LOCK-FILES.txt
date >> /tmp/LOCK-FILES.txt
for i in `find /flood-db -name '*.L'`; do
   echo $i |tee -a /tmp/LOCK-FILES.txt
   rm $i
done
echo "LOCK files have been deleted, confirming via 'find'"
for i in `find /flood-db -name '*.L'`; do echo $i; done

